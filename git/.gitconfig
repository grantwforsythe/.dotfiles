[alias]
    amend = commit --amend --reuse-message=HEAD
    graph =  git log --all --decorate --oneline --graph

[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold

[color "status"]
    added = yellow
    changed = green
    untracked = cyan


